<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gcash1noback"
    tools:context=".Gcash1">

    <Button
        android:id="@+id/GLoginButton"
        android:layout_width="345dp"
        android:layout_height="65dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="#5271ff"
        android:clickable="true"
        android:onClick="navigateToGcash2Page"
        android:text="Log in"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/no1"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="292dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no3"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="292dp"
        android:layout_marginEnd="44dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/no2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no4"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="44dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/no1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no5"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="44dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num5"
        app:layout_constraintStart_toEndOf="@+id/no4"
        app:layout_constraintTop_toBottomOf="@+id/no2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no6"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="44dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/no5"
        app:layout_constraintTop_toBottomOf="@+id/no3"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no7"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="52dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/no4"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no8"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="168dp"
        android:layout_marginTop="52dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/no4"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no9"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="44dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toEndOf="@+id/no8"
        app:layout_constraintTop_toBottomOf="@+id/no6"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no2"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="292dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num2"
        app:layout_constraintStart_toEndOf="@+id/no1"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/no0"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginEnd="151dp"
        android:layout_marginBottom="7dp"
        android:clickable="true"
        android:onClick="onNumericButtonClick"
        android:src="@drawable/num0"
        app:layout_constraintBottom_toTopOf="@+id/GLoginButton"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/dotPass1"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="99dp"
        android:layout_marginTop="223dp"
        android:src="@drawable/dotpass"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/dotPass2"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="223dp"
        android:src="@drawable/dotpass"
        app:layout_constraintStart_toEndOf="@+id/dotPass1"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/dotPass3"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="223dp"
        android:src="@drawable/dotpass"
        app:layout_constraintStart_toEndOf="@+id/dotPass2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/dotPass4"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="223dp"
        android:src="@drawable/dotpass"
        app:layout_constraintStart_toEndOf="@+id/dotPass3"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>